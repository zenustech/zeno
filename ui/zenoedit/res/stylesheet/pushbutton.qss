QPushButton[cssClass="grayButton"]
{
	border-style: outset;
    border-width: 2px;
    border-radius: 1px;
	border-color: #000000;
	background-color: #343A42;
	font-size: 10pt;;
	font-weight: 500;
	color: #737B85;
}

QPushButton[cssClass="grayButton"]:hover
{
	color: rgb(255, 255, 255);
}

QPushButton[cssClass="blueStyle"]
{
	padding-top: 2.5px;
	padding-left: 12px;
	padding-right: 12px;
	padding-bottom: 2.5px;

    border-width: 0px;
    border-radius: 0px;
	background-color: #4284F4;
	font-size: 10pt;;
	font-weight: 500;
	color: rgb(208, 208, 208);
}

QPushButton[cssClass="blueStyle"]:hover
{
	color: rgb(255, 255, 255);
}

QPushButton[cssClass="dictkeypanel"]
{
	padding-top: 2.5px;
	padding-left: 48px;
	padding-right: 48px;
	padding-bottom: 2.5px;

    border-width: 0px;
    border-radius: 5px;
	border-style: outset;
	background-color: #5A616D;
	font-size: 10pt;;
	font-weight: bold;
	color: #A3B1C0;
}

QPushButton[cssClass="dictkeypanel"]:hover
{
